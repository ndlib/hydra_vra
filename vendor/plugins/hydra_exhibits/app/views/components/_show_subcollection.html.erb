<fieldset id="item_fields">
  <legend>Item Description</legend>
    <dl>
      <% unless get_values_from_datastream(@document_fedora,"descMetadata", [:dsc, :collection, :did, :unittitle]).first.empty?%>
         <dt class="title">
           <%= fedora_field_label("descMetadata", [:dsc, :collection, :did, :unittitle], "Title:")%>
	     </dt>
         <dd>
           <%= get_values_from_datastream(@document_fedora,"descMetadata", [:dsc, :collection, :did, :unittitle])%>
         </dd>
      <%end%>
      <% unless get_values_from_datastream(@document_fedora,"descMetadata", [:dsc, :collection, :did, :unitid]).first.empty?%>
