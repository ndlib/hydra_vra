<fieldset id="subcollection_fields">
  <legend>Subcollection</legend>
    <dl>
      <dt>
        <p> Parent: </p>
      </dt>
      <dd class="parent">
        <%- parent_id = @document_fedora.member_of_ids.first -%>
        <%- parent = Collection.load_instance_from_solr(parent_id)-%>
        <%= link_to "#{parent.title}", catalog_path(parent_id, :exhibit_id => params[:exhibit_id], :render_search => params[:render_search], :viewing_context => params[:viewing_context], :f => params[:f]) %>
      </dd>
      <% unless get_values_from_datastream(@document_fedora,"descMetadata", [:collection, :did, :unittitle, :unittitle_content]).first.empty?%>
         <dt class="title">
           <%= fedora_field_label("descMetadata", [:collection, :did, :unittitle, :unittitle_content], "Title:")%>
	     </dt>
         <dd>
           <%= get_values_from_datastream(@document_fedora,"descMetadata", [:collection, :did, :unittitle, :unittitle_content])%>
         </dd>
      <%end%>
      <% unless get_values_from_datastream(@document_fedora,"descMetadata", [:collection, :did, :unitid]).first.empty?%>
          <dt class="abbreviated_title">
            <%= fedora_field_label("descMetadata", [:collection, :did, :unitid], "Abbreviated Title:") %>
          </dt>
          <dd>
            <%= get_values_from_datastream(@document_fedora,"descMetadata", [:collection, :did, :unitid])%>
          </dd>
      <%end%>
      <% unless get_values_from_datastream(@document_fedora,"descMetadata", [:collection, :did, :unitid, :unitid_identifier]).first.empty?%>
          <dt class="subcollection_id">
            <%= fedora_field_label("descMetadata", [:collection, :did, :unitid, :unitid_identifier], "ID:") %>
          </dt>
          <dd>
            <%= get_values_from_datastream(@document_fedora,"descMetadata", [:collection, :did, :unitid, :unitid_identifier])%>
          </dd>
      <%end%>
      <% unless get_values_from_datastream(@document_fedora,"descMetadata",  [:collection, :did, :origination, :printer]).first.empty?%>
          <dt class="printer">
            <%= fedora_field_label("descMetadata", [:collection, :did, :origination, :printer], "Printer:") %>
          </dt>
          <dd>
            <%= get_values_from_datastream(@document_fedora,"descMetadata", [:collection, :did, :origination, :printer])%>
          </dd>
      <%end%>
      <% unless get_values_from_datastream(@document_fedora,"descMetadata", [:collection, :did, :origination, :engraver]).first.empty?%>
         <dt class="engraver">
           <%= fedora_field_label("descMetadata", [:collection, :did, :origination, :engraver], "Engraver:")%>
	 </dt>
         <dd>
           <%= get_values_from_datastream(@document_fedora,"descMetadata", [:collection, :did, :origination, :engraver])%>
         </dd>
     <%end%>
      <% unless get_values_from_datastream(@document_fedora,"descMetadata", [:collection, :did, :unittitle, :imprint, :geogname]).first.empty?%>
         <dt class="printing_location">
           <%= fedora_field_label("descMetadata", [:collection, :did, :unittitle, :imprint, :geogname], "Printing Location:")%>
	 </dt>
         <dd>
           <%= get_values_from_datastream(@document_fedora,"descMetadata", [:collection, :did, :unittitle, :imprint, :geogname])%>
         </dd>
     <%end%>
      <% unless get_values_from_datastream(@document_fedora,"descMetadata", [:collection, :did, :unittitle, :imprint, :publisher]).first.empty?%>
         <dt class="publisher">
