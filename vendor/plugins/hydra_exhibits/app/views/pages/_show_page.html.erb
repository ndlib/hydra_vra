<fieldset id="page_fields">
  <legend>Page</legend>
    <dl>
      <dt>
        <p>Parent: </p>
      </dt>
      <dd class="parent">
        <%- parent_id = @document_fedora.item_ids.first -%>
        <%- parent = Component.load_instance_from_solr(parent_id)-%>
        <%= link_to "#{parent.title}", catalog_path(parent_id, :exhibit_id => params[:exhibit_id], :render_search => params[:render_search], :viewing_context => params[:viewing_context], :f => params[:f]) %>
      </dd>
      <% unless get_values_from_datastream(@document_fedora,"properties", [:name]).first.empty?%>
          <dt class="name">
            <%= fedora_field_label("properties", [:name], "Name:") %>
          </dt>
          <dd>
            <%= @document_fedora.name %>
          </dd>
      <%end%>
      <% unless get_values_from_datastream(@document_fedora,"properties", [:title]).first.empty?%>
          <dt class="title">
            <%= fedora_field_label("properties", [:title], "Title:") %>
          </dt>
          <dd>
            <%= @document_fedora.title %>
          </dd>
      <%end%>
    </dl>
</fieldset>


