<h3><%=h facet_value if facet_value %> <em><%= grouped_result_count(@response, facet_name, facet_value) %></em></h3>
<table>
	<% unless params[:display_type] == 'list' %>
  <tr>
    <th class="title">Title</th>
    <th class="type">Type</th>
    <th class="type">Highlighted in Collection</th>
    <th class="type">Highlighted in Subcollection</th>

  </tr>
  <% end %>
  <% docs.each_with_index do |document,counter| %>
   <%= render_item_partial document, 'edit_highlighted', :counter => counter %>
  <% end %>
</table>