<form id="document_metadata" action="<%= url_for(:action=>"update", :controller=>"sub_collections", :content_type=>content)%>" method="post" data-resourceType="<%= content %>" accept-charset="utf-8" data-pid="<%= params[:id] %>" data-content-type="<%= content %>">

  <fieldset id="work_fields">
    <legend> Items </legend>
    <dl class="item-list">
      <%= get_search_results_from_params %>
    </dl>
    <a class='addhighlighted' href='#' title='add as highlighted' collection-id="<%= params[:collection_id] %>" >Add as highlighted</a>
    <%# debug params.inspect %>
  </fieldset>
  <%= render :partial => "shared/edit_description", :locals => {:content => 'collection', :document_fedora => @document_fedora} %>  
</form>
